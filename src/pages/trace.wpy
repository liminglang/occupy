<style>
  .container{
    position:absolute;
    top:0;
    bottom:0;
    left:0;
    right:0;
    display:flex;
    flex-direction:column;
    align-items:center;
    justify-content:space-between;
    box-sizing:border-box;

  }
  .circleBar {
  width: 100%;
  overflow: hidden;
  position: relative;
  margin-bottom:70px;
  }
  
  .cir {
    display: inline-block;
    background-color: #fff;
    border-radius: 100%;
  }
  
  .top {
    text-align: center;
  }
  .button{
    position:fixed;
    top:2%;
    left:-23px;
    width:40%;
    height:8%;
  }
  .button2{
    width: 40%;
    background:transparent;
    border-radius: 35px;	
    color:#FD562A;
    font-size: 14px;
    margin-bottom: 10%;
