.photo {
  margin-top: 20px;
  width: 60%;
  height: 150px;
  margin-left: auto;
  margin-right: auto;
  border: solid 1px #000;
}
.confirm {
  margin-top: 20px;
  height: 30px;
  width: 80px;
  display: table;
}
.gps {
  height: 30px;
  width: 30px;
}
.image {
  width: 50px;
  height: 50px;
  border-radius: 50%;
}
.name {
  flex: 1;
  margin-left: 10px;
}
.button {
  display: flex;
  flex-direction: column;
  align-items: center;
  display: inline;
  background-color: #ffffff;
  margin-left: 30px;
}
